{
	"schemaVersion": 1,
	"id": "${id}",
	"version": "${version}",
	"name": "${name}",
	"description": "${desc}",
	"authors": [
		{
			"name": "axialeaa",
			"contact": {
				"homepage": "https://github.com/axialeaa"
			}
		}
	],
	"contributors": [],
	"contact": {
		"homepage": "https://modrinth.com/mod/${modrinth}",
		"sources": "https://github.com/${repo}",
		"issues": "https://github.com/${repo}/issues"
	},
	"license": "LGPLv3",
	"icon": "assets/${id}/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.axialeaa.deepslate_dungeons.DeepslateDungeons"
		],
		"fabric-datagen": [
			"com.axialeaa.deepslate_dungeons.data.DeepslateDungeonsDataGenerator"
		]
	},
	"accessWidener" : "${id}.classtweaker",
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.github_releases": "https://github.com/${repo}/releases",
				"modmenu.modrinth": "https://modrinth.com/mod/${id}"
			}
		}
	},
	"mixins": [
		"${id}.mixins.json"
	],
	"depends": {
		"fabricloader": ">=${loader}",
		"minecraft": "${target}",
		"java": ">=${java}",
		"fabric": "*"
	}
}